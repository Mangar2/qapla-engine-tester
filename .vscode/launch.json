{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Qapla Debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/qapla-engine-tester",
        "args": [
          "--logpath=./test/log",
          "--enginelog",
          "--test",
          "numgames=0",
          "nostop",
          "nooption",
          "--engine",
          "conf=Qapla 0.3.2",
          "--enginesfile=./test/linux-engines.ini"
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }
  